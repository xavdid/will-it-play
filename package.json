{
  "name": "will-it-play",
  "version": "1.0.1",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": "lib/cli.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "validate": "yarn test && yarn lint",
    "release": "npx np"
  },
  "keywords": [
    "playstaion",
    "ps5",
    "ps4"
  ],
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "author": {
    "name": "David Brownman",
    "email": "beamneocube@gmail.com",
    "url": "https://xavd.id"
  },
  "repository": "github:xavdid/will-it-play",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "chalk": "4.1.2",
    "cli-table3": "0.6.0",
    "commander": "8.3.0",
    "fluent-ffmpeg": "2.1.2"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "2.1.18",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-standard": "5.0.0",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  }
}
